	array6 = array1;
	array7 = array1;
	array8 = array1;
	array9 = array1;
	array10 = array1;
	
	String [][] tablero = new String[][] {array1, array2, array3, array4, array5,
			                              array6, array7, array8, array9, array10};

	
	for (int x=0; x < tablero.length; x++) {
		
		  for (int y=0; y < tablero[x].length; y++) {
			  
						  
		    if (y!=tablero[x].length-1) System.out.print("\t");
		  }
		 
		}
	
	
	
