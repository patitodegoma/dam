package ejercicio_10;

public class Entrada {

	//Atributos 
	
	private int id;
	private int fila;
	private int asiento;
	
	// Constructores
	
	public Entrada () {
		this.id = 0;
		this.fila = 0;
		this.asiento = 0;
	}
	
	public Entrada (int fila, int asiento) {
		this.fila = fila;
		this.asiento = asiento;
	}
	
	public Entrada (int id, int fila, int asiento) {
		this.id = id;
		this.fila = fila;
		this.asiento = asiento;
	}

	// Getters y setters
	
	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public int getFila() {
		return fila;
	}

	public void setFila(int fila) {
		this.fila = fila;
	}

	public int getAsiento() {
		return asiento;
	}

	public void setAsiento(int asiento) {
		this.asiento = asiento;
	}
	
	
	// Métodos
	
	
	
	
}
